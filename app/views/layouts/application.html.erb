<!DOCTYPE html>
<html>
<head>

  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div class="icon">
		<%= link_to image_tag("/ada_logo.png", size: "238x100"), root_url %>
	</div>
	<div class="top-bar-right">
		<img src="/top_bar_right2.png" height="100" width="279.25"/>
	</div>
	<div class="top-bar-middle">
	</div>
    <% if user_signed_in? %>
		<div class="container">
			<ul class="nav nav-tabs">
              <li role="presentation"> <%=link_to "Profile", current_user %> </li>
			  <li role="presentation"> <%= link_to "Home", root_url %></li>
			  <li role="presentation"> <%= link_to "Account", edit_user_registration_path %></li>
			  <li role="presentation"> <%= link_to "Sign Out", destroy_user_session_path, method: :delete %>
			  </li>
			</ul>
	    </div>
    <% end %>

<%= yield %>

</body>
</html>
